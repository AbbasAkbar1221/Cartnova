{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build",
          "buildCommand": "npm run build",
          "outputDirectory": "frontend/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/build/$1"
      }
    ]
  }
  